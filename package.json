{
  "name": "Management-fe-RS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "build:report": "vue-cli-service build --report --mode production && cd dist && live-server --open=./report.html",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint **/*.{html,vue} --custom-syntax postcss-html",
    "lint:css": "stylelint **/*.{css,sass,scss} --syntax scss",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-rc.12",
    "await-timeout": "^1.1.1",
    "axios": "^0.21.1",
    "core-js": "^3.15.2",
    "dayjs": "^1.10.5",
    "lodash-es": "^4.17.21",
    "material-design-icons-iconfont": "^6.1.0",
    "nprogress": "^0.2.0",
    "vue": "^2.6.14",
    "vue-img-inputer": "^2.1.6",
    "vue-router": "^3.5.2",
    "vue-snotify": "^3.2.1",
    "vuetify": "^2.5.5",
    "vuex": "^3.6.2",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.9",
    "lint-staged": "^9.5.0",
    "live-server": "^1.2.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mockjs-async": "^0.0.5",
    "sass": "^1.35.1",
    "sass-loader": "^10.2.0",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^13.13.1",
    "stylelint-codeframe-formatter": "^1.0.4",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^1.1.0",
    "terser-webpack-plugin": "^2.2.1",
    "vue-template-compiler": "^2.6.14"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
