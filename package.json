{
  "name": "Management-fe-RS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build:dev": "vue-cli-service build --mode development",
    "build:pro": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint **/*.{html,vue} --custom-syntax postcss-html",
    "lint:css": "stylelint **/*.{css,sass,scss} --syntax scss",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "await-timeout": "^0.6.0",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "lodash": "^4.17.15",
    "material-design-icons-iconfont": "^5.0.1",
    "moment": "^2.24.0",
    "secure-ls": "^1.2.5",
    "vue": "^2.6.10",
    "vue-fragment": "^1.5.1",
    "vue-img-inputer": "^2.1.6",
    "vue-router": "^3.1.3",
    "vue-snotify": "^3.2.1",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "cz-customizable": "^6.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "lodash-webpack-plugin": "^0.11.5",
    "mockjs-async": "^0.0.5",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "style-resources-loader": "^1.2.1",
    "stylelint": "^12.0.0",
    "stylelint-codeframe-formatter": "^1.0.4",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.1.0",
    "terser-webpack-plugin": "^2.2.1",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
